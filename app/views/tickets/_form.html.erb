
<%= form_for Ticket.new, url: {action: "create"} do |f| %>
  <%= f.label :first_name %>:
  <%= f.text_field :first_name, :placeholder =>"First Name" %><br />
  <%= f.label :last_name %>:
  <%= f.text_field :last_name %><br />
  <%= f.label :email_address %>
  <%= f.text_field :email_address %><br />
  <%= f.label :credit_card_number %>:
  <%= f.text_field :credit_card_number%><br />
  <%= f.label :credit_cart_expiration_date %>:
  <%= f.text_field :credit_cart_expiration_date%><br />

  <%= f.submit "See the Show" %>
<% end %>
